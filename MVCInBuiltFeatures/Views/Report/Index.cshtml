
@{
    ViewBag.Title = "View";
}

<h2>View</h2>

@model IEnumerable<MVCInBuiltFeatures.Models.SResult>

@{
    ViewBag.Title = "State Area";
}
<h2>State Area - Report</h2>

<table>
    <tr>
        <th>
           
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date)
        </th>


        <th>
            @Html.DisplayNameFor(model => model.present_illness)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.vital_sign)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.result)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.medicine)
        </th>

        <th></th>
    </tr>

        @foreach (var item in Model)
        {
<tr>
           
            <td>@Html.DisplayFor(modelItem => item.date)</td>
            <td>@Html.DisplayFor(modelItem => item.present_illness)</td>
    <td>
        @Html.DisplayFor(modelItem => item.vital_sign)
    </td>

    <td>
        @Html.DisplayFor(modelItem => item.result)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.medicine)
    </td>
            
        </tr>
    }
</table>


<div style="padding:10px; border:1px solid black">
    <div><a href="@Url.Action("Report",new {id= "PDF"})"> Get Report PDF</a></div>
    <div><a href="@Url.Action("Report",new {id= "Excel"})"> Get Report Excel</a></div>
    <div><a href="@Url.Action("Report",new {id= "Word"})"> Get Report Word</a></div>
    <div><a href="@Url.Action("Report",new {id= "Image"})"> Get Report Image</a></div>
</div>